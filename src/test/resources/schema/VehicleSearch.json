{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": ["count", "next", "previous", "results"],
  "properties": {
    "count": {
      "type": "integer",
      "minimum": 0
    },
    "next": {
      "type": ["string", "null"],
      "format": "uri"
    },
    "previous": {
      "type": ["string", "null"],
      "format": "uri"
    },
    "results": {
      "type": "array",
      "minItems": 0,
      "items": {
        "type": "object",
        "required": [
          "id",
          "status",
          "vehicle_type",
          "model",
          "state",
          "coach_layout",
          "created_at",
          "updated_at",
          "name",
          "registration_number",
          "chassis_number",
          "registration_date",
          "is_state_registered",
          "owner_id",
          "added_by",
          "updated_by"
        ],
        "properties": {
          "id": {
            "type": "integer",
            "minimum": 1
          },
          "status": {
            "type": "string",
            "minLength": 1
          },

          "vehicle_type": {
            "type": "object",
            "required": ["id", "name"],
            "properties": {
              "id": { "type": "integer", "minimum": 1 },
              "name": { "type": "string", "minLength": 1 }
            }
          },

          "model": {
            "type": "object",
            "required": ["id", "name"],
            "properties": {
              "id": { "type": "integer", "minimum": 1 },
              "name": { "type": "string", "minLength": 1 }
            }
          },

          "state": {
            "type": "object",
            "required": ["id", "name"],
            "properties": {
              "id": { "type": "integer", "minimum": 1 },
              "name": { "type": "string", "minLength": 1 }
            }
          },

          "coach_layout": {
            "type": "object",
            "required": ["id", "name"],
            "properties": {
              "id": { "type": "integer", "minimum": 1 },
              "name": { "type": "string", "minLength": 1 }
            }
          },

          "created_at": {
            "type": "string",
            "format": "date-time"
          },
          "updated_at": {
            "type": "string",
            "format": "date-time"
          },
          "deleted_at": {
            "type": ["string", "null"],
            "format": "date-time"
          },

          "name": {
            "type": "string",
            "minLength": 1
          },
          "name_meta": { "type": "string" },
          "name_paytm": { "type": "string" },
          "name_redbus": { "type": "string" },

          "registration_number": {
            "type": "string",
            "minLength": 1
          },
          "chassis_number": {
            "type": "string",
            "minLength": 1
          },
          "registration_date": {
            "type": "string",
            "pattern": "^\\d{4}-\\d{2}-\\d{2}$"
          },

          "fuel_type_id": { "type": "integer" },
          "vehicle_color_id": { "type": "integer" },
          "rc_validity_date": {
            "type": "string",
            "pattern": "^\\d{4}-\\d{2}-\\d{2}$"
          },

          "is_state_registered": {
            "type": "boolean"
          },

          "manufacture_year": { "type": "integer" },
          "fuel_tank_capacity": { "type": "integer" },
          "fuel_tank_type": { "type": "integer" },
          "fuel_tank_measurement": { "type": "integer" },

          "run_km": {
            "type": "number",
            "minimum": 0
          },

          "owner_id": {
            "type": "integer",
            "minimum": 1
          },

          "veh_cost_per_km": { "type": "number" },
          "veh_score": { "type": "number" },
          "funnel_status": { "type": "integer" },
          "version": { "type": "integer" },

          "added_by": {
            "type": "integer",
            "minimum": 1
          },
          "updated_by": {
            "type": "integer",
            "minimum": 1
          },

          "axle_type": { "type": "integer" },
          "mg_type": { "type": "integer" },
          "mg_per_seat": { "type": "number" },

          "ticket_revenue_share": { "type": "number" },
          "non_ticket_revenue_share": { "type": "number" },

          "obd": {
            "type": "string"
          },

          "revenue_share": { "type": "number" },
          "profit_share": { "type": "number" }
        }
      }
    }
  }
}
