{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "count": {
      "type": "integer",
      "minimum": 0
    },
    "next": {
      "type": ["string", "null"],
      "format": "uri"
    },
    "previous": {
      "type": ["string", "null"],
      "format": "uri"
    },
    "results": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "id": { "type": "integer" },

          "city": {
            "type": "object",
            "properties": {
              "id": { "type": "integer" },
              "name": { "type": "string" },
              "latitude": { "type": "string" },
              "longitude": { "type": "string" }
            },
            "required": ["id", "name", "latitude", "longitude"]
          },

          "created_at": {
            "type": "string",
            "format": "date-time"
          },
          "updated_at": {
            "type": "string",
            "format": "date-time"
          },
          "deleted_at": {
            "type": ["string", "null"],
            "format": "date-time"
          },

          "name": { "type": "string" },
          "version": { "type": "integer" },
          "area_name": { "type": "string" },
          "address": { "type": "string" },
          "latitude": { "type": "string" },
          "longitude": { "type": "string" },
          "status": { "type": "integer" },
          "bitla_ref_id": { "type": "integer" },

          "address_book_type": {
            "type": "string",
            "enum": ["LOADING_UNLOADING"]
          },
          "address_book_category": {
            "type": "string",
            "enum": ["BRANCH"]
          }
        },
        "required": [
          "id",
          "city",
          "created_at",
          "updated_at",
          "name",
          "version",
          "area_name",
          "address",
          "latitude",
          "longitude",
          "status",
          "address_book_type",
          "address_book_category"
        ]
      }
    }
  },
  "required": ["count", "results"]
}
