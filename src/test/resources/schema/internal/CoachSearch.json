{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": ["count", "next", "previous", "results"],
  "properties": {
    "count": {
      "type": "integer"
    },
    "next": {
      "type": ["string", "null"]
    },
    "previous": {
      "type": ["string", "null"]
    },
    "results": {
      "type": "array",
      "items": {
        "type": "object",
        "required": [
          "id",
          "coach_seats",
          "created_at",
          "updated_at",
          "name",
          "num_row",
          "num_col",
          "z_index_count",
          "status",
          "version",
          "layout_data",
          "total_seats"
        ],
        "properties": {
          "id": {
            "type": "integer"
          },
          "coach_seats": {
            "type": "array",
            "items": {
              "type": "object",
              "required": [
                "id",
                "created_at",
                "updated_at",
                "col",
                "row",
                "z_index",
                "seat_type",
                "seat_num",
                "is_ladies",
                "view_seat_type",
                "view_seat_height",
                "view_seat_width",
                "bitla_ref_id",
                "version",
                "added_by",
                "updated_by",
                "coach"
              ],
              "properties": {
                "id": { "type": "integer" },
                "created_at": { "type": "string" },
                "updated_at": { "type": "string" },
                "deleted_at": { "type": ["string", "null"] },
                "col": { "type": "integer" },
                "row": { "type": "integer" },
                "z_index": { "type": "integer" },
                "seat_type": { "type": "string" },
                "seat_sub_type": { "type": ["string", "null"] },
                "seat_num": { "type": "string" },
                "is_ladies": { "type": "boolean" },
                "view_seat_type": { "type": "string" },
                "view_seat_height": { "type": "integer" },
                "view_seat_width": { "type": "integer" },
                "bitla_ref_id": { "type": "integer" },
                "layout": { "type": ["integer", "null"] },
                "seat_position": { "type": ["string", "null"] },
                "version": { "type": "integer" },
                "added_by": { "type": "integer" },
                "updated_by": { "type": "integer" },
                "coach": { "type": "integer" }
              }
            }
          },
          "created_at": { "type": "string" },
          "updated_at": { "type": "string" },
          "deleted_at": { "type": ["string", "null"] },
          "name": { "type": "string" },
          "num_row": { "type": "integer" },
          "num_col": { "type": "integer" },
          "z_index_count": { "type": "integer" },
          "status": { "type": "integer" },
          "bitla_ref_id": { "type": "integer" },
          "version": { "type": "integer" },
          "added_by": { "type": "integer" },
          "updated_by": { "type": "integer" },
          "layout_data": {
            "type": "string",
            "description": "JSON string representing layout configuration"
          },
          "description": { "type": "string" },
          "total_seats": { "type": "integer" }
        }
      }
    }
  }
}
