{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "status": {
      "type": "object",
      "properties": {
        "http_status_code": {
          "type": "integer",
          "enum": [200]
        }
      },
      "required": ["http_status_code"],
      "additionalProperties": false
    },
    "data": {
      "type": "object",
      "properties": {
        "Amenities": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "id": {
                "type": "integer"
              },
              "name": {
                "type": "string"
              }
            },
            "required": ["id", "name"],
            "additionalProperties": false
          },
          "minItems": 1
        }
      },
      "required": ["Amenities"],
      "additionalProperties": false
    }
  },
  "required": ["status", "data"],
  "additionalProperties": false
}
