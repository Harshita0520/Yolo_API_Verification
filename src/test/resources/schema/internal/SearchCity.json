{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "status": {
      "type": "object",
      "properties": {
        "http_status_code": {
          "type": "integer"
        }
      },
      "required": ["http_status_code"]
    },
    "data": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "_source": {
            "type": "object",
            "properties": {
              "id": {
                "type": "integer"
              },
              "name": {
                "type": "string"
              },
              "weightage": {
                "type": "integer"
              },
              "tags": {
                "type": "array",
                "items": {
                  "type": "string"
                }
              }
            },
            "required": ["id", "name", "weightage", "tags"]
          }
        },
        "required": ["_source"]
      }
    }
  },
  "required": ["status", "data"]
}
