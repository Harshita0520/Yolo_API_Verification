{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": [
    "id",
    "city",
    "created_at",
    "updated_at",
    "name",
    "version",
    "area_name",
    "address",
    "latitude",
    "longitude",
    "status",
    "bitla_ref_id",
    "address_book_type",
    "address_book_category"
  ],
  "properties": {
    "id": {
      "type": "integer"
    },
    "city": {
      "type": "object",
      "required": ["id", "name", "latitude", "longitude"],
      "properties": {
        "id": {
          "type": "integer"
        },
        "name": {
          "type": "string"
        },
        "latitude": {
          "type": "string"
        },
        "longitude": {
          "type": "string"
        }
      }
    },
    "created_at": {
      "type": "string"
    },
    "updated_at": {
      "type": "string"
    },
    "deleted_at": {
      "type": ["string", "null"]
    },
    "name": {
      "type": "string"
    },
    "version": {
      "type": "integer"
    },
    "area_name": {
      "type": "string"
    },
    "address": {
      "type": "string"
    },
    "latitude": {
      "type": "string"
    },
    "longitude": {
      "type": "string"
    },
    "status": {
      "type": "integer"
    },
    "bitla_ref_id": {
      "type": "integer"
    },
    "address_book_type": {
      "type": "string",
      "enum": ["LOADING_UNLOADING"]
    },
    "address_book_category": {
      "type": "string",
      "enum": ["BRANCH"]
    }
  }
}
